<h2><%= pageTitle %></h2>
<a href="/books/add" class="btn btn-info">Add New Book</a>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Author</th>
      <th>Tags</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% for( let index = 0; index < book.length; index++ ) { %>
    <tr>
      <td><%=book[index].title %></td>
      <td><%=book[index].year %></td>
      <td><%=book[index].author %></td>
      <td><%=book[index].tags %></td>
      <td>
        <a
          href="/books/edit/<%=book[index]._id  %>"
          class="btn btn-warning btn-sm"
          >Edit</a
        >
      </td>
      <td>
        <a
          href="/books/delete/<%=book[index]._id  %>"
          class="btn btn-danger btn-sm"
          >Delete</a
        >
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
